{
  "name": "vipre",
  "version": "1.2.3",
  "private": true,
  "author": "NASA JPL",
  "description": "A tool for visualizing the entry conditions for all safe entry opportunities of a planetary entry probe being released from an interplanetary trajectory.",
  "homepage": "./",
  "main": "./public/electron.js",
  "build": {
    "appId": "com.electron.vapre",
    "productName": "VIPRE",
    "mac": {
      "category": "public.app-category.business",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      "vipre-data"
    ]
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^3.0.5",
    "@devexpress/dx-react-grid": "^3.0.5",
    "@devexpress/dx-react-grid-material-ui": "^3.0.5",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.2",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.4.0",
    "@mui/styles": "^5.10.3",
    "@mui/system": "^5.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.26.1",
    "d3-scale-chromatic": "^3.0.0",
    "find-process": "^1.4.7",
    "material-design-icons": "^3.0.1",
    "mathjs": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-globe.gl": "^2.22.0",
    "react-reorder": "^3.0.0-alpha.7",
    "react-scripts": "5.0.0",
    "recharts": "^2.1.9",
    "three": "^0.143.0",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.4.2",
    "usehooks-ts": "^2.4.2",
    "web-vitals": "^2.1.0",
    "zustand": "^3.7.0",
    "zustand-persist": "^0.4.0"
  },
  "scripts": {
    "install-api": "STARTDIR=`pwd` && cd vipre-data && poetry install",
    "start-api": "STARTDIR=`pwd` && cd vipre-data/src && poetry run uvicorn --reload --log-level debug app.main:app --port 8000",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "build": "yarn version && yarn react-build && yarn electron-build",
    "electron-build": "electron-builder -mwl",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "setupFiles": "./src/setupTests.ts"
  },
  "electronmon": {
    "patterns": [
      "!dist/**",
      "!build/**"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/three": "^0.142.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-is-dev": "^2.0.0",
    "electronmon": "^2.0.2",
    "node-sass": "^7.0.1",
    "sass": "^1.49.7",
    "wait-on": "^6.0.0"
  }
}
